<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Age:</strong>
  <%= @user.age %>
</p>

<% if @fileuploads.count > 0 %>
	<%= render template: "fileuploads/index" %>
<% end %>
<% if @filedownloads.count > 0 %>
	<%= render template: "filedownloads/index" %>
<% end %>
<% if current_user.id == @user.id %>
  <%= link_to 'Edit', edit_user_path(@user) %> |
<% end %>
<%= link_to 'Back', :back %>
