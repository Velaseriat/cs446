<h1>Listing filefantasies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filefantasies.each do |filefantasy| %>
      <tr>
        <td><%= filefantasy.name %></td>
        <td><%= link_to 'Show', filefantasy %></td>
        <td><%= link_to 'Edit', edit_filefantasy_path(filefantasy) %></td>
        <td><%= link_to 'Destroy', filefantasy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filefantasy', new_filefantasy_path %>
